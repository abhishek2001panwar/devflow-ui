{
  "name": "devflow-ui",
  "version": "0.1.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ui": "tsup src/lib/ui/index.ts --dts --format esm,cjs --out-dir dist",
    "prepublishOnly": "npm run build:ui",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "publish:patch": "npm version patch --no-git-tag-version && npm publish",
    "publish:minor": "npm version minor --no-git-tag-version && npm publish",
    "publish:major": "npm version major --no-git-tag-version && npm publish"
  },
  "dependencies": {
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.3.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0"
  }
}
